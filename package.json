	"scripts": {
		"dev": "cross-env NODE_ENV=development webpack -d",
		"build:css": "node-sass src/ -o src/ ",
		"watch:css": "npm run build:css && node-sass src/ -o src/ -w -r",
		"prod": "cross-env NODE_ENV=production webpack -p",
		"devstart": "yarn dev && yarn start",
		"devstart2": "yarn dev && yarn wpds",
		"prodstart": "yarn prod && yarn start",
		"start": "yarn gui-server",
		"gui-server": "yarn http-server -p 8899 ./dist/ -d --gzip -c10 -o http://localhost:8899/test-pages/list.html",
		"run-data-server": "node ./server-mock/app.js",
		"watch": "cross-env NODE_ENV=watch webpack -d",
		"clean": "rm -rf ./dist",
		"clean:all": "rm -rf ./dist && rm -rf ./coverage && rm -rf ./node_modules",
		"clean:dist": "rm -rf ./dist",
		"test": "jest",
		"test:coverage": "jest --collect-coverage",
		"wpds": "webpack-dev-server --open"
	},
